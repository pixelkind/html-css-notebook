{
  "name": "html-css-notebook",
  "displayName": "HTML & CSS Notebook",
  "description": "A notebook extension for easy HTML & CSS development",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "keywords": [
    "notebookRenderer",
    "teaching",
    "notebook",
    "education"
  ],
  "categories": [
    "Education",
    "Notebooks",
    "Visualization",
    "Other"
  ],
  "activationEvents": [
    "onNotebook:html-css-notebook"
  ],
  "main": "./out/extension/extension.js",
  "browser": "./out/extension/extension.web.js",
  "contributes": {
    "notebooks": [
      {
        "type": "html-css-notebook",
        "displayName": "HTML & CSS Notebook",
        "selector": [
          {
            "filenamePattern": "*.html-css-notebook"
          }
        ]
      }
    ],
    "notebookRenderer": [
      {
        "id": "io.garrit.html-css-notebook",
        "entrypoint": "./out/client/index.js",
        "displayName": "HTML & CSS Notebook",
        "mimeTypes": [
          "text/html"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "lint": "eslint src --ext ts",
    "watch": "webpack --mode development --watch",
    "pretest": "webpack --mode development && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/webpack-env": "^1.16.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@types/vscode-notebook-renderer": "^1.60.0",
    "@types/vscode": "^1.71.0",
    "css-loader": "^4.2.0",
    "eslint": "^8.14.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.4",
    "vscode-notebook-error-overlay": "^1.0.1",
    "@vscode/test-electron": "^2.1.3",
    "util": "^0.12.4",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  }
}
